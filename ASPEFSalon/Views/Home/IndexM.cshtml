@model IEnumerable<EFSalon.Master>
        @{ ViewBag.Title = "Все мастера"; }
        <a asp-action="CreateС">Добавить мастера</a>
        <table class="table">
            <tr><td>Имя</td><td>Возраст</td></tr>
            @foreach (var item in Model)
            {
<tr>
    <td>@item.Name</td>
    <td>@item.Surname</td>
    <td>@item.Gender</td>
</tr>
}
        </table>
